{
  "name": "stsbroker",
  "description": "CLI to configure and interact with your own AWS STS Broker.",
  "version": "0.0.0",
  "author": "nascit",
  "bin": {
    "stsbroker": "./bin/run"
  },
  "bugs": "https://github.com/sts-broker-cli/stsbroker/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "axios": "^0.19.2",
    "cli-ux": "^5.4.5",
    "inquirer": "^7.0.4",
    "node-persist": "^3.0.5",
    "tslib": "^1.11.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/chai": "^4.2.9",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.16",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/sts-broker-cli/stsbroker",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "stsbroker",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "sts-broker-cli/stsbroker",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
